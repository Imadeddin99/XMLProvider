<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="CalculatorSuite" verbose="1" >
    <test name="Addition" >
        <parameter name="number1" value="25"/>
        <parameter name="expectedResult" value="5"/>
        <classes>
            <class name="XMLSquareRoot" />
        </classes>
    </test>
    <test name="Addition1" >
        <parameter name="number1" value="2.4"/>
        <parameter name="expectedResult" value="1.549"/>
        <classes>
            <class name="XMLSquareRoot" />
        </classes>
    </test>
    <test name="Addition2" >
        <parameter name="number1" value="0"/>
        <parameter name="expectedResult" value="0"/>
        <classes>
            <class name="XMLSquareRoot" />
        </classes>
    </test>






</suite>
