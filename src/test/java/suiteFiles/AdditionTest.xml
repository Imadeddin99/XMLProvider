<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="CalculatorSuite" verbose="1" >
    <test name="Addition" >
        <parameter name="number1" value="5"/>
        <parameter name="number2" value="7"/>
        <parameter name="expectedResult" value="12"/>
        <classes>
            <class name="XMLAdditionTest" />
        </classes>
    </test>
    <test name="Addition1" >
        <parameter name="number1" value="2.4"/>
        <parameter name="number2" value="5.3"/>
        <parameter name="expectedResult" value="7.7"/>
        <classes>
            <class name="XMLAdditionTest" />
        </classes>
    </test>
    <test name="Addition2" >
        <parameter name="number1" value="5"/>
        <parameter name="number2" value="-2"/>
        <parameter name="expectedResult" value="3"/>
        <classes>
            <class name="XMLAdditionTest" />
        </classes>
    </test>
    <test name="Addition3" >
        <parameter name="number1" value="-5.4"/>
        <parameter name="number2" value="2.6"/>
        <parameter name="expectedResult" value="-2.8"/>
        <classes>
            <class name="XMLAdditionTest" />
        </classes>
    </test>
    <test name="Addition4" >
        <parameter name="number1" value="-5.8"/>
        <parameter name="number2" value="0"/>
        <parameter name="expectedResult" value="-5.8"/>
        <classes>
            <class name="XMLAdditionTest" />
        </classes>
    </test>




</suite>
